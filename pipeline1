node{
     stage('build'){
     echo "welcome to build stage"
   }
     stage('test'){
     echo "welcome to test stage"
   }
     stage('qa'){
     echo "welcome to qa stage"
   }
     stage('deploy'){
     echo "welcome to deploy stage"
   }


}

node {
    stage('Example') {
        if (env.BRANCH_NAME == 'master') {
            echo 'I only execute on the master branch'
        } else {
            echo 'I execute elsewhere'
        }
    }
}

